                        "type": "string",
                        "nullable": true
                    },
                    "houseNumber": {
                        "type": "string",
                        "nullable": true
                    },
                    "city": {
                        "type": "string",
                        "nullable": true
                    },
                    "country": {
                        "type": "string",
                        "nullable": true
                    },
                    "zipcode": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "RequestGeoLocationByText": {
                "required": [
                    "text"
                ],
                "type": "object",
                "properties": {
                    "text": {
                        "minLength": 3,
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "RequestRoute": {
                "required": [
                    "waypoints"
                ],
                "type": "object",
                "properties": {
                    "traffic": {
                        "$ref": "#/components/schemas/Traffic"
                    },
                    "waypoints": {
                        "minItems": 2,
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/WaypointModel"
                        }
                    },
                    "returnWaypoints": {
                        "type": "boolean"
                    }
                },
                "additionalProperties": false
            },
            "RoadAccessPosition": {
                "type": "object",
                "properties": {
                    "latitude": {
                        "type": "number",
                        "format": "double"
                    },
                    "longitude": {
                        "type": "number",
                        "format": "double"
                    }
                },
                "additionalProperties": false
            },
            "RoutingResponse": {
                "type": "object",
                "properties": {
                    "totalDistance": {
                        "type": "number",
                        "format": "double"
                    },
                    "totalTollDistance": {
                        "type": "number",
                        "format": "double"
                    },
                    "travelTime": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "totalTollCosts": {
                        "type": "number",
                        "format": "double"
                    },
                    "waypoints": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/WaypointModel"
                        },
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "Traffic": {
                "enum": [
                    0
                ],
                "type": "integer",
                "format": "int32"
            },
            "WaypointModel": {
                "required": [
                    "latitude",
                    "longitude"
                ],
                "type": "object",
                "properties": {
                    "latitude": {
                        "type": "number",
                        "format": "double"
                    },
                    "longitude": {
                        "type": "number",
                        "format": "double"
                    }
                },
                "additionalProperties": false
            }
        },
